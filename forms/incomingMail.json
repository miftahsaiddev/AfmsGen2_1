                        {
                            "className": "org.joget.apps.form.lib.CustomHTML",
                            "properties": {
                                "requiredSanitize": "true",
                                "autoPopulate": "",
                                "label": "",
                                "id": "field23",
                                "value": "<script>\r\ndocument.getElementsByName('status_order')[0].addEventListener('change', function() {\r\n  var textField = document.getElementsByName('no_project')[0];\r\n  \r\n  // Contoh: membuat textfield menjadi readonly jika opsi yang dipilih adalah \"opsi2\"\r\n  if (this.value === '0') {\r\n    textField.readOnly = true; // membuat textfield readonly\r\n  } else {\r\n    textField.readOnly = false; // membuat textfield bisa diedit\r\n  }\r\n});\r\n\r\n\r\nvar callback_project = {\r\n    success: function(response) {\r\n        console.log(\"response masuk patent: \"+response);\r\n        var responseObject = JSON.parse(response);\r\n        var fletter_from = FormUtil.getField(\"letter_from\");\r\n        var fclient_ref = FormUtil.getField(\"client_ref\");\r\n        var fletter_type = FormUtil.getField(\"letter_type\");\r\n        var freceipt_via = FormUtil.getField(\"receipt_via\");\r\n        var fagent = FormUtil.getField(\"agent\");\r\n        var vagent='';\r\n        // var fagent_address = FormUtil.getField(\"agent_address\");\r\n        document.getElementsByName('letter_from')[0].disabled = false;\r\n        document.getElementsByName('client_ref')[0].readOnly = false;\r\n        document.getElementsByName('letter_type')[0].disabled = false;\r\n        document.getElementsByName('receipt_via')[0].disabled = false;\r\n        document.getElementsByName('agent')[0].disabled = false;\r\n        // document.getElementsByName('agent_address')[0].readOnly = false;\r\n        $(fletter_from).val(\"\"); \r\n        $(fclient_ref).val(\"\"); \r\n        $(fletter_type).val(\"\"); \r\n        $(freceipt_via).val(\"\"); \r\n        $(fagent).val(\"\"); \r\n        // $(fagent_address).val(\"\"); \r\n        responseObject.forEach(function(item) {\r\n            document.getElementsByName('letter_from')[0].disabled = true;\r\n            document.getElementsByName('client_ref')[0].readOnly = true;\r\n            document.getElementsByName('letter_type')[0].disabled = true;\r\n            document.getElementsByName('receipt_via')[0].disabled = true;\r\n            document.getElementsByName('agent')[0].disabled = true;\r\n            document.getElementsByName('agent_address')[0].readOnly = true;\r\n            $(fletter_from).val(item.c_letter_from); \r\n            $(fclient_ref).val(item.c_client_ref); \r\n            // $(fletter_type).val(item.c_letter_type);  \r\n            $(freceipt_via).val(item.c_receipt_via); \r\n            $(fagent).val(item.c_client); \r\n            vagent=item.c_client;\r\n        });\r\n        console.log('url agent : http://localhost:8080/afms-Api/getClientDetailById/'+vagent);\r\n        \r\n        ConnectionManager.get('http://localhost:8080/afms-Api/getClientDetailById/'+vagent, callback_agent, '', false);\r\n        \r\n    }\r\n};\r\n\r\nvar callback_agent = {\r\n    success: function(response) {\r\n        console.log(\"response masuk agent: \"+response);\r\n        var responseObject = JSON.parse(response);\r\n        var fagent_address = FormUtil.getField(\"agent_address\");\r\n        $(fagent_address).val(\"\"); \r\n        responseObject.forEach(function(item) {\r\n            $(fagent_address).val(item.c_address); \r\n        });\r\n        \r\n    }\r\n};\r\n    \r\n$(\"#no_project\").blur(function(){\r\n    var values = FormUtil.getValues(\"no_project\");\r\n    var valfield=''\r\n    for (var i = 0; i < values.length; i++) {\r\n        console.log(values[i]);\r\n        valfield =values[i];\r\n    }\r\n    ConnectionManager.get('http://localhost:8080/afms-Api/getPatentDetailById/'+valfield, callback_project, '', false);\r\n});\r\n\r\n\r\ndocument.addEventListener('DOMContentLoaded', function() {\r\n    // Select the main select element by its specific identifier (e.g., id or name)\r\n    var mainSelectElement = document.querySelector('select[name=\"letter_from\"]');\r\n    \r\n    // Function to control the display of other select elements based on the main select's value\r\n    function controlSelectDisplays() {\r\n        // Initially set all select elements to display none\r\n        var agentSelect = document.querySelector('select[name=\"agent\"]');\r\n\t\tvar labelToHideAgent = document.querySelector('label[field-tooltip=\"agent\"]');\r\n        var agentAddressSelect = document.querySelector('textarea[name=\"agent_address\"]');\r\n\t\tvar labelToHideAgentAddress = document.querySelector('label[for=\"agent_address\"]');\r\n        var governmentSelect = document.querySelector('select[name=\"government\"]');\r\n\t\tvar labelToHideGovernment = document.querySelector('label[field-tooltip=\"government\"]');\r\n        var translatorSelect = document.querySelector('select[name=\"translator\"]');\r\n\t\tvar labelToHideTranslator = document.querySelector('label[field-tooltip=\"translator\"]');\r\n        \r\n        if(agentSelect) agentSelect.style.display = 'none';\r\n\t\tif(labelToHideAgent) labelToHideAgent.style.display = 'none';\r\n        if(agentAddressSelect) agentAddressSelect.style.display = 'none';\r\n\t\tif(labelToHideAgentAddress) labelToHideAgentAddress.style.display = 'none';\r\n        if(governmentSelect) governmentSelect.style.display = 'none';\r\n        if(labelToHideGovernment) labelToHideGovernment.style.display = 'none';\r\n        if(translatorSelect) translatorSelect.style.display = 'none';\r\n\t\tif(labelToHideTranslator) labelToHideTranslator.style.display = 'none';\r\n\r\n        // Check the value of the main select and adjust display properties accordingly agent_address\r\n        if (mainSelectElement.value === 'LF-001') {\r\n            if(agentSelect) agentSelect.style.display = 'block';\r\n\t\t\tif(labelToHideAgent) labelToHideAgent.style.display = 'block';\r\n\t\t\tif(agentAddressSelect) agentAddressSelect.style.display = 'block';\r\n\t\t\tif(labelToHideAgentAddress) labelToHideAgentAddress.style.display = 'block';\r\n        } else if (mainSelectElement.value === 'LF-002') {\r\n            if(governmentSelect) governmentSelect.style.display = 'block';\r\n\t\t\tif(labelToHideGovernment) labelToHideGovernment.style.display = 'block';\r\n        } else if (mainSelectElement.value === 'LF-004') {\r\n            if(translatorSelect) translatorSelect.style.display = 'block';\r\n\t\t\tif(labelToHideTranslator) labelToHideTranslator.style.display = 'block';\r\n        }else{}\r\n    }\r\n\r\n    // Add change event listener to the main select element\r\n    mainSelectElement.addEventListener('change', controlSelectDisplays);\r\n\r\n    // Initial call to set the correct display on page load\r\n    controlSelectDisplays();\r\n});\r\n    \r\n<\/script>\r\n"
                            }
                        }
